@base <http://example.cubeviz.org/datacube/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .

@prefix cube: <http://purl.org/linked-data/cube#> .
@prefix ex: <http://example.cubeviz.org/datacube/> .

<> a owl:Ontology ;
   rdfs:label "Example DataCube Knowledge Base" ;
   dc:description "This knowledgebase contains one Data Structure Definition with one Data Set. This Data Set has a couple of Components and Observations." .

# 
# Data Structure Definitions
#
ex:dsd a cube:DataStructureDefinition ;
    rdfs:label "A Data Structure Definition"@en ;
    rdfs:comment "Defines the structure of a DataSet or slice." ;
    cube:component  <http://example.cubeviz.org/datacube/dsd/geo>, 
                    <http://example.cubeviz.org/datacube/dsd/time>,
                    <http://example.cubeviz.org/datacube/dsd/unit>, 
                    <http://example.cubeviz.org/datacube/dsd/value> .
                    
ex:dsd2 a cube:DataStructureDefinition ;
    rdfs:comment "Defines the structure of a DataSet or slice." ;
    cube:component  <http://example.cubeviz.org/datacube/dsd/geo>, 
                    <http://example.cubeviz.org/datacube/dsd/time>,
                    <http://example.cubeviz.org/datacube/dsd/unit>, 
                    <http://example.cubeviz.org/datacube/dsd/value> .

#
# Data Set
#
ex:dataset a cube:DataSet ;
    rdfs:label "A DataSet"^^<http://www.w3.org/2001/XMLSchema#string> ;
    rdfs:comment "Represents a collection of observations and conforming to some common dimensional structure." ;
    cube:structure ex:dsd .
    
ex:dataset2 a cube:DataSet ;
    rdfs:label "Just another DataSet"^^<http://www.w3.org/2001/XMLSchema#string> ;
    rdfs:comment "Represents a collection of observations and conforming to some common dimensional structure." ;
    cube:structure ex:dsd2 .
    
ex:dataset3 a cube:DataSet ;
    cube:structure ex:dsd2 .

#
# Component Specifications
#
<http://example.cubeviz.org/datacube/dsd/geo> a cube:ComponentSpecification ;
    rdfs:label "Component Specification of Region" ;
    cube:dimension ex:geo .

<http://example.cubeviz.org/datacube/dsd/time> a cube:ComponentSpecification ;
    rdfs:label "Component Specification of Time" ;
    cube:dimension ex:date .

<http://example.cubeviz.org/datacube/dsd/unit> a cube:ComponentSpecification ;
    rdfs:label "Component Specification of Unit" ;
    cube:attribute ex:unit .

<http://example.cubeviz.org/datacube/dsd/value> a cube:ComponentSpecification ;
    rdfs:label "Component Specification of Value" ;
    cube:measure ex:value .

#
# Dimensions, Unit and Measure
#
ex:geo a cube:DimensionProperty ;
    rdfs:label "Region"@en .
        
ex:date a cube:DimensionProperty ;
    rdfs:label "Date"@en .

ex:unit a cube:AttributeProperty ;
    rdfs:label "Unit" .

ex:value a cube:MeasureProperty ;
    rdfs:label "Value"@en .
    
#
# Observation #################################################################
#

#
# Data Set 1
#
ex:obs1 a cube:Observation ;
    cube:dataSet ex:dataset ;
    ex:date ex:Y2001 ;
    ex:unit "whatever it is about" ;
    ex:geo ex:Germany ;
    ex:value "1500"^^<http://www.w3.org/2001/XMLSchema#float> ;
    rdfs:label "Germany in 2001" .

ex:obs2 a cube:Observation ;
    cube:dataSet ex:dataset ;
    ex:date ex:Y2002 ;
    ex:unit "whatever it is about" ;
    ex:geo ex:Germany ;
    ex:value "2000"^^<http://www.w3.org/2001/XMLSchema#float> ;
    rdfs:label "Germany in 2002" .

ex:obs3 a cube:Observation ;
    cube:dataSet ex:dataset ;
    ex:date ex:Y2001 ;
    ex:unit "whatever it is about" ;
    ex:geo ex:England ;
    ex:value "1400"^^<http://www.w3.org/2001/XMLSchema#float> ;
    rdfs:label "England in 2001" .

ex:obs4 a cube:Observation ;
    cube:dataSet ex:dataset ;
    ex:date ex:Y2002 ;
    ex:unit "whatever it is about" ;
    ex:geo ex:England ;
    ex:value "1999"^^<http://www.w3.org/2001/XMLSchema#float> ;
    rdfs:label "England in 2002" .

#
# Data Set 2
#
ex:obs5 a cube:Observation ;
    cube:dataSet ex:dataset2 ;
    ex:date ex:Y2003 ;
    ex:unit "whatever it is about" ;
    ex:geo ex:England ;
    ex:value "1998"^^<http://www.w3.org/2001/XMLSchema#float>.

ex:obs6 a cube:Observation ;
    cube:dataSet ex:dataset2 ;
    ex:date ex:Y2003 ;
    ex:unit "whatever it is about" ;
    ex:geo ex:Germany ;
    ex:value "1998"^^<http://www.w3.org/2001/XMLSchema#float>.

#
# Data Set 3
#

ex:obs7 a cube:Observation ;
    cube:dataSet ex:dataset3 ;
    ex:date ex:Y2004 ;
    ex:unit "whatever it is about" ;
    ex:geo ex:Germany ;
    ex:value "1998"^^<http://www.w3.org/2001/XMLSchema#float>.

ex:obs8 a cube:Observation ;
    cube:dataSet ex:dataset3 ;
    ex:date ex:Y2004 ;
    ex:unit "whatever it is about" ;
    ex:geo ex:Germany ;
    ex:value "1998"^^<http://www.w3.org/2001/XMLSchema#float>.

#
# Additional Stuff #############################################################
#
ex:Germany a ex:Region ;
    rdfs:label "Germany"@en .
    
ex:England a ex:Region ;
    rdfs:label "England"@en .
       
ex:Y2001 a ex:Time ;
    rdfs:label "2001"@en .

ex:Y2002 a ex:Time ;
    rdfs:label "2002"@en .

ex:Y2003 a ex:Time ;
    rdfs:label "2003"@en .

ex:Y2004 a ex:Time .
